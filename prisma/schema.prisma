generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id          Int      @id @default(autoincrement())
  items       String   // JSON string representing the list of items
  totalPrice  Float
  pickupTime  String
  customerName String  @default("Cliente") 
  notes       String?
  status      String   @default("In attesa")
  createdAt   DateTime @default(now())
}
